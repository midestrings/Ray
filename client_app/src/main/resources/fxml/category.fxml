<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: #2E3033; -fx-background-radius: 5px;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ray.app.controller.CategoryCardController">
   <children>
      <HBox alignment="BASELINE_CENTER" layoutX="10.0" layoutY="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <ImageView fx:id="image" fitHeight="111.0" fitWidth="230.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../assets/images/car.jpg" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <Label fx:id="name" layoutX="21.0" layoutY="123.0" prefHeight="25.0" prefWidth="149.0" textFill="WHITE" AnchorPane.leftAnchor="10.0">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <TextFlow layoutX="5.0" layoutY="151.0" prefHeight="46.0" prefWidth="238.0">
         <children>
            <Text fx:id="description" fill="#eeeeee" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
      </TextFlow>
   </children>
</AnchorPane>
